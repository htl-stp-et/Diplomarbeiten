% ---------------------------------------------------------------------------
% Einbinden von PCX-Bildern:
% ---------------------------------------------------------------------------
\def\pcxget#1#2#3#4#5{\hbox to #2{\hskip#3%   zum Einbinden von PCX-Files
\vbox to #4{\vskip#5\special{em:graph #1}\vss}\hss}}
\def\pcx#1#2#3#4#5%  Einlesen von PCX-Files, HAA,15.10.1993
{\parbox{#2}{\vspace*{#5}\hskip#3\special{em:graph #1}%
\vspace{-#5}\vspace{#4}}}
\def\pcxt#1#2#3#4#5%  Einlesen von PCX-Files, HAA,15.10.1993
{\parbox[t]{#2}{\vspace*{#5}\hskip#3\special{em:graph #1}%
\vspace{-#5}\vspace{#4}}}
\def\pcxb#1#2#3#4#5%  Einlesen von PCX-Files, HAA,15.10.1993
{\parbox[b]{#2}{\vspace*{#5}\hskip#3\special{em:graph #1}%
\vspace{-#5}\vspace{#4}}}

% ---------------------------------------------------------------------------
% Positionierung von Texten:
% ---------------------------------------------------------------------------

\newbox\insertbox
\long\def\position#1#2#3{{\global\setbox\insertbox\hbox{#3}\unskip%
\hskip#2\makebox[0pt][l]{%
\vtop to0pt{\vskip#1\vskip-\ht\insertbox\copy\insertbox\vss}}%
\unskip\hskip-#2}}%
\long\def\positionc#1#2#3{{\global\setbox\insertbox\hbox{#3}\unskip%
\hskip#2\makebox[0pt][c]{%
\vtop to0pt{\vskip#1\vskip-\ht\insertbox\copy\insertbox\vss}}%
\unskip\hskip-#2}}%
\def\xposition#1#2{\makebox[0mm][l]{\hskip#1#2}}

