\newdimen\ypos\newdimen\xpos
\long\def\Label#1#2#3{%
   \ypos37.125mm\multiply\ypos by#1\advance\ypos by -37.125mm
   \xpos70.000mm\multiply\xpos by#2\advance\xpos by -70.000mm
   \advance\ypos by21.00mm\advance\xpos by 5mm
   \position{\ypos}{\xpos}{$\vcenter{
   \begin{minipage}{60mm}#3\end{minipage}}$}}

