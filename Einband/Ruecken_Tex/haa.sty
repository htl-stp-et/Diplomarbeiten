% ---------------------------------------------------------------------------
% Tabulatoraehnliche Umgebung (aus KOMA)                             1.7.1996
% ---------------------------------------------------------------------------
\newenvironment{tabulator}[2][]
               {\def\sc@septext{#1}
                \list{}{\settowidth{\labelwidth}{#2#1}
                        \leftmargin\labelwidth \advance\leftmargin by \labelsep
                        \let\makelabel\tabulatorlabel}}
               {\endlist}
\newenvironment{Zerotabulator}[2][]
               {\def\sc@septext{#1}
                \list{}{\topsep0pt\itemsep0pt\parsep0pt\settowidth{\labelwidth}{#2#1}
                        \leftmargin\labelwidth \advance\leftmargin by \labelsep
                        \let\makelabel\tabulatorlabel}}
               {\endlist}
\newcommand\tabulatorlabel[1]{#1\hfil\sc@septext}

